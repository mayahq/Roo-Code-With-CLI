{
	"name": "roo-cli",
	"version": "3.11.9",
	"description": "Command Line Interface for Roo",
	"main": "dist/cli.js",
	"type": "module",
	"bin": {
		"roo": "dist/cli.js",
		"roo-config": "dist/ipc-bridge-cli/index.js"
	},
	"scripts": {
		"build": "tsc && chmod +x dist/cli.js dist/core/repl.js dist/ipc-bridge-cli/index.js",
		"start": "node dist/cli.js",
		"dev": "tsc --watch & node --watch dist/cli.js",
		"postinstall": "chmod +x dist/cli.js dist/core/repl.js dist/ipc-bridge-cli/index.js || true"
	},
	"keywords": [
		"cli",
		"ai",
		"agent",
		"vscode",
		"extension"
	],
	"author": "Maya",
	"license": "UNLICENSED",
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/commander": "^2.12.2",
		"@types/dotenv": "^8.2.0",
		"@types/marked": "^6.0.0",
		"@types/node": "^20.12.12",
		"@types/yargs": "^17.0.32",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.22.0",
		"axios": "^1.8.4",
		"chalk": "^5.3.0",
		"commander": "^12.1.0",
		"dotenv": "^16.4.7",
		"marked": "^13.0.2",
		"node-ipc": "^12.0.0",
		"yargs": "^17.7.2"
	}
}
